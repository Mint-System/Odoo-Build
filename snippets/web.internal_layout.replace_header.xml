<?xml version="1.0"?>
<data inherit_id="web.internal_layout" priority="50">>

	<xpath expr="//div[@class='header']" position="replace">
	<div class="header">
	  <table style="width:100%;">
	    <tr>
	      <td><img t-if="company.logo" t-att-src="image_data_uri(company.logo)" alt="Logo"  style="height:61px; float:right"/></td>
	    </tr>
	  </table>
	</div>
	</xpath>

</data>