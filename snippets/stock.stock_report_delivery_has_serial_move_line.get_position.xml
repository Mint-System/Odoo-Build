<?xml version="1.0"?>
<data inherit_id="stock.stock_report_delivery_has_serial_move_line" priority="50">

    <xpath expr="//td[1]" position="before">
         <t t-if="o.sale_id or o.purchase_id">
            <td id="position">
                <span t-esc="move_line.move_id.position"/>
            </td>
         </t>
    </xpath>

</data>