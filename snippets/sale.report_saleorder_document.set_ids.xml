<?xml version="1.0"?>
<data inherit_id="sale.report_saleorder_document" priority="60">

	<xpath expr="//table[2]//th[1]" position="attributes">
		<attribute name="id">position</attribute>
	</xpath>
	<xpath expr="//table[2]//td[1]" position="attributes">
		<attribute name="id">position</attribute>
	</xpath>

	<xpath expr="//table[2]//th[2]" position="attributes">
		<attribute name="id">description</attribute>
	</xpath>

	<xpath expr="//table[2]//th[3]" position="attributes">
		<attribute name="id">quantity</attribute>
	</xpath>
	<xpath expr="//table[2]//td[3]" position="attributes">
		<attribute name="id">quantity</attribute>
	</xpath>
	<xpath expr="//table[2]//td[3]/span[1]" position="attributes">
		<attribute name="id">qty</attribute>
	</xpath>

</data>