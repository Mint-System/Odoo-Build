<?xml version="1.0"?>
<data inherit_id="sale_blanket_order.report_blanketorder_document" priority="50">
  
  <xpath expr="//tbody[hasclass('sale_tbody')]//tr" position="attributes">
    <attribute name="class" separator=" " add="first"/>
  </xpath>

  <xpath expr="//tbody[hasclass('sale_tbody')]//tr" position="after">
    <tr class="second">
      <td></td>
      <td></td>
      <td colspan="4">
        <span t-field="l.name" />
        <br />
        <t t-if="l.product_id.country_of_origin_id.code">
          Ursprungsland:
          <span t-field="l.product_id.country_of_origin_id.code" />
        </t>
        <t t-if="l.product_id.hs_code">
          / Zollposition:
          <span t-field="l.product_id.hs_code" />
        </t>
      </td>
      <td></td>
    </tr>
  </xpath>

</data>