<?xml version="1.0"?>
<data inherit_id="sale.report_saleorder_document" priority="50">
    <xpath expr="//th[@name='th_subtotal']" position="after">
        <th id="tax_amount">
            <span/>
        </th>
    </xpath>
    <xpath expr="//td[@name='td_subtotal']" position="after">
        <td id="tax_amount">
            <span t-out="line.tax_id.amount"/>
        </td>
    </xpath>
</data>
