<?xml version="1.0"?>
<data inherit_id="account.report_invoice_document">

  <xpath expr="/t/t/div/table/thead/tr/th[1]" position="before">
    <th>
      <span>Pos</span>
    </th>
  </xpath>
  <xpath expr="//t[@t-foreach='lines']" position="before">
    <t t-set="index" t-value="1"/>
  </xpath>
  <xpath expr="/t/t/div/table/tbody/t[4]/tr/t[1]/td[1]" position="before">
    <td>
      <!-- <span t-esc="line.sequence - 9"/> -->
      <span t-esc="index"/><t t-set="index" t-value="index+1"/>
    </td>
  </xpath>

</data>
