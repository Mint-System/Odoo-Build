<?xml version="1.0"?>
<data inherit_id="stock.report_delivery_document">

<xpath expr="//div/table[1]/tbody/tr/td[2]/span" position="after">
    <t t-if="o.product_id.hs_code">
      <br/>
      HS Code: <span t-field="o.product_id.hs_code"/>
    </t>  
    <t t-if="o.product_id.x_country_of_origin.code">
      <br/>
      Country of Origin: <span t-field="o.product_id.x_country_of_origin.code"/>
    </t>
    <t t-if="move.product_id.weight">
      <br/>
      Weight: <span t-field="move.product_id.weight"/>
      <span t-field="move.product_id.weight_uom_id.display_name"/>
    </t>
  </xpath>
  
</data>