<t t-name="studio_main_report">
    <t t-foreach="docs" t-as="doc">
        <t t-call="web.basic_layout">
            <t t-if="doc.product_id.x_product_label.name == 'St端cketikette Spheretex'">
                <t t-call="studio_customization.studio_report_docume_84b2d998-cc56-45cc-8ec2-9b375a1bbcd0_document"/>
            </t>
            
            <t t-if="doc.product_id.x_product_label.name == 'St端cketikette Standard'">
                <t t-call="studio_customization.studio_report_docume_5322cd1b-2c35-419d-8e1f-1e7afb5936e3_document"/>
            </t>
            
            <t t-if="doc.product_id.x_product_label.name == 'St端cketikette neutral'">
                <t t-call="studio_customization.studio_report_docume_2b92a163-f467-4a96-92ea-963ba7b0c2d2_document"/>
            </t>
            
            <t t-if="doc.product_id.x_product_label.name == 'St端cketikette Sika'">
                <t t-call="studio_customization.studio_report_docume_4fd86ea4-bacb-4603-96e7-7cfa5773b052_document"/>
            </t>
            
            <t t-if="doc.product_id.x_product_label.name == 'Etikette Ketten'">
                <t t-call="studio_customization.studio_report_docume_69399d9a-c835-4e10-a933-6a4cd82a854d_document"/>
            </t>
            
            <p style="page-break-after: always;"/>
        </t>
 
    </t>
</t>

