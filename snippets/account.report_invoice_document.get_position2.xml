        <th id="position" class="text-start">
            <span>Pos.</span>
        </th>
    </xpath>
    <xpath expr="//table[@name='invoice_line_table']//t[1]/td[1]" position="before">
        
        <td id="position">
            <span t-esc="line.sequence"/>
        </td>
    </xpath>
</data>