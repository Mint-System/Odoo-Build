<?xml version="1.0"?>
<data inherit_id="sale.report_saleorder_document" priority="60">
    
    <th id="qty" position="replace">
        <t t-if="is_confirmation">
            <span t-field="line.product_uom_qty" />
        </t>
        <t t-else="">
            <span t-field="line.product_uom_qty" />
        </t>
    </th>

    <span id="product_uom_qty" position="replace">
        <t t-if="is_confirmation">
            <span id="product_uom_qty_is_confirmation" t-field="line.product_uom_qty" />
        </t>
        <t t-else="">
            <span id="product_uom_qty_is_not_confirmation" t-field="line.product_uom_qty" />
        </t>
    </span>

</data>